<div class="container">
  <div class="col-md-8">
    <% @my_bookings.each do |booking| %>
      <% if booking.user_id == current_user.id %>
        <div class="bookings-container">
          <div class="bookings-dates">
            <p>Congrats, this is your booking!</p>
            <%= booking.date_begin %> | <%= booking.date_end %> |
            <p>the total price is: <%= (booking.date_end - booking.date_begin).to_i * booking.device.price %>â‚¬</p>
            <% if booking.status == true %>
            <p> accepted ! </p>
            <% end %>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>

